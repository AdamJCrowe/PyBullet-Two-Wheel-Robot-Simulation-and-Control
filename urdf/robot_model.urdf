<?xml version="1.0"?>
<robot name="robot_system">

  <material name="body_mat">
    <color rgba="0 0 0.6 1"/>
  </material>

  <material name="wheel_mat">
    <color rgba="1 0.65 0 1"/>
  </material>
  
  <link name="base_link" />

  <link name="body">
    <visual>
      <geometry>
        <box size="0.151 0.06 0.285"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="body_mat"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.151 0.06 0.285"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.765"/>
      <inertia ixx="0.0206" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.00251"/>
	  <origin rpy="0 0 0" xyz="0 0 -0.114"/>
    </inertial>
  </link>
  
   <joint name="base_to_chassis" type="fixed">
      <parent link="base_link"/>
      <child link="body"/>
      <origin xyz="0 0 0"/>
   </joint>

  <link name="wheel_a">
    <visual>
      <geometry>
        <cylinder length="0.004" radius="0.0405"/>
      </geometry>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
      <material name="wheel_mat"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.004" radius="0.0405"/>
      </geometry>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.03"/>
      <inertia ixx="0.0000246" ixy="0.0" ixz="0.0" iyy="0.000013" iyz="0.0" izz="0.000013"/>
	  <origin rpy="0 1.5708 0" xyz="0 0 0"/>
    </inertial>
  </link>

  <link name="wheel_b">
    <visual>
      <geometry>
        <cylinder length="0.004" radius="0.0405"/>
      </geometry>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
      <material name="wheel_mat"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.004" radius="0.0405"/>
      </geometry>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.03"/>
      <inertia ixx="0.0000246" ixy="0.0" ixz="0.0" iyy="0.000012" iyz="0.0" izz="0.000012"/>
	  <origin rpy="0 1.5708 0" xyz="0 0 0"/>
    </inertial>
  </link>

  <joint name="wheel_a_shaft" type="continuous">
    <parent link="body"/>
    <child link="wheel_a"/>
    <axis xyz="1 0 0"/>
    <origin rpy="0 0 0" xyz="0.0845 0 -0.114"/>
  </joint>

  <joint name="wheel_b_shaft" type="continuous">
    <parent link="body"/>
    <child link="wheel_b"/>
    <axis xyz="1 0 0"/>
	<origin rpy="0 0 0" xyz="-0.0845 0 -0.114"/>
  </joint>

</robot>